<?php

include_once 'User.php';

/**
 * Classe che rappresenta un Operatore
 *
 * @author Davide Spano
 */
class Operatore extends User {


    public function __construct() {
        // richiamiamo il costruttore della superclasse
        parent::__construct();
        $this->setRuolo(User::Operatore);
    }


}

?>
